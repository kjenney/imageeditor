{
  "name": "imageeditor",
  "version": "0.1.0",
  "private": true,
  "description": "A web-based image editor application",
  "keywords": [
    "image",
    "editor",
    "canvas",
    "photo",
    "react",
    "typescript"
  ],
  "homepage": "https://github.com/kjenney/imageeditor#readme",
  "bugs": {
    "url": "https://github.com/kjenney/imageeditor/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kjenney/imageeditor.git"
  },
  "license": "MIT",
  "author": "",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,css,json}\"",
    "type-check": "tsc --noEmit",
    "docs": "typedoc",
    "docs:watch": "typedoc --watch",
    "docs:serve": "typedoc && npx serve api-docs",
    "prepare": "husky"
  },
  "dependencies": {
    "konva": "^9.3.18",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-konva": "^19.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.14",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "lint-staged": "^16.2.7",
    "prettier": "^3.3.3",
    "serve": "^14.2.4",
    "typedoc": "^0.27.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.15.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,css}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
